<%= form_tag signin_path do %>
    <label> Your Name: </label>
    <%= select_tag "user_name", options_from_collection_for_select(@users, "name", "name"), :prompt=>"Please select your username from this dropdown" %><br><br>

    <label> Your Password: </label>
    <%= password_field_tag "password" %><br>

    <%= submit_tag "Sign In" %>
<% end %>