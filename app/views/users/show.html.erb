
<p class="message"><%= flash[:message] %></p>
<p class="error-message"><%= flash[:error] %></p>

<% if @current_user.admin %>
<p>ADMIN</p>
<% end %>

<p>Name: <%= @current_user.name %></p>
<p>Tickets: <%= @current_user.tickets %></p>
<p>Mood: <%= @current_user.mood %></p>
<p>Height: <%= @current_user.height %></p>

<div><%= link_to "See attractions", attractions_path %></div>
<div><%= link_to "Log Out", logout_path %></div>